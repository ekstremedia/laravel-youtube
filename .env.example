# YouTube API Configuration
# Get these from https://console.cloud.google.com
YOUTUBE_CLIENT_ID=
YOUTUBE_CLIENT_SECRET=
YOUTUBE_REDIRECT_URI=/youtube/callback

# Admin Panel Settings
YOUTUBE_ADMIN_ENABLED=true
YOUTUBE_ADMIN_PREFIX=youtube-admin

# Default Settings
YOUTUBE_DEFAULT_PRIVACY=private
YOUTUBE_DEFAULT_CATEGORY=22
YOUTUBE_DEFAULT_LANGUAGE=en

# Rate Limiting
YOUTUBE_RATE_LIMIT_ENABLED=true

# Logging
YOUTUBE_LOGGING_ENABLED=true
YOUTUBE_LOG_CHANNEL=youtube
YOUTUBE_LOG_LEVEL=info

# Upload Settings
YOUTUBE_UPLOAD_CHUNK_SIZE=1048576
YOUTUBE_UPLOAD_TIMEOUT=3600
YOUTUBE_MAX_FILE_SIZE=137438953472